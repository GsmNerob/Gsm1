package com.upskill.java_1;

public class DataType {

	public static void main(String[] args) {
		
		//there are 2 types of data type:
		//	a) Primitive Data type- int, bolean, short, long, boolean, float
		//	b) Non Primitive Data Type - string class, object, array
		
		System.out.println("Hello World, This is Rasel");
		
		int a = 585243;
		int b = 1790;
		int c = a + b;
		System.out.println(c);
		
		String city = "New York";
		System.out.println(city);	
		
		boolean upskill = true;
		System.out.println(upskill);
	}
}
