package com.skill.assigmant_1;

public class DoWhile_loops {

	
	public static void main(String[] args) {
		//practiceDoWhileLoop();
		//practiceDoWhileLoopOne();
		practiceInfiniteLoop();
	}
		
		public static void practiceDoWhileLoop(){
			int i = 1;
			do{
				System.out.println("Do While Lopps Number =" + i);
	        i++;
	    	}while (i<=80);	
		}
	
		
		public static void practiceDoWhileLoopOne(){
			int i = 100;
			do{
				System.out.println("Do While Loops Number =" + i);
				i--;
			}while(i>=10);
			
				
    	}  
         public static void practiceInfiniteLoop(){
	        int i;
	        for (i = 1; ; i++){
		        System.out.println(" for Loops Number =" + i);
	        }
       }
         }